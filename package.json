{
  "name": "@angular-devkit/devkit",
  "version": "0.0.0",
  "description": "Software Development Kit for Angular",
  "bin": {
    "devkit-admin": "./bin/devkit-admin",
    "schematics": "./bin/schematics"
  },
  "keywords": [
    "angular",
    "devkit",
    "sdk",
    "Angular DevKit"
  ],
  "scripts": {
    "admin": "node ./bin/devkit-admin",
    "build": "npm run admin -- build",
    "build-tsc": "tsc -p tsconfig.json",
    "fix": "npm run lint -- --fix",
    "lint": "tslint --config tslint.json --project tsconfig.json --type-check",
    "test": "node ./bin/devkit-admin test",
    "validate-commits": "./bin/devkit-admin validate-commits",
    "integration": "npm run build && npm run integration:build-optimizer",
    "integration:build-optimizer": "npm run integration:build-optimizer:simple && npm run integration:build-optimizer:aio",
    "integration:build-optimizer:simple": "cd tests/@angular_devkit/build_optimizer/webpack/simple-app && npm i && npm run reinstall-bo && npm run e2e && npm run benchmark",
    "integration:build-optimizer:aio": "cd tests/@angular_devkit/build_optimizer/webpack/aio-app && npm i && npm run reinstall-bo && npm run e2e && npm run benchmark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/devkit.git"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "author": "Angular Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angular/devkit/issues"
  },
  "homepage": "https://github.com/angular/devkit",
  "dependencies": {
    "@ngtools/json-schema": "^1.0.9",
    "@ngtools/logger": "^1.0.1",
    "@types/common-tags": "^1.2.4",
    "@types/glob": "^5.0.29",
    "@types/istanbul": "^0.4.29",
    "@types/jasmine": "^2.5.47",
    "@types/minimist": "^1.2.0",
    "@types/node": "^6.0.36",
    "@types/semver": "^5.3.30",
    "@types/source-map": "^0.5.0",
    "@types/webpack": "^3.0.2",
    "common-tags": "^1.3.1",
    "conventional-changelog": "^1.1.0",
    "glob": "^7.0.3",
    "istanbul": "^0.4.5",
    "jasmine": "^2.6.0",
    "jasmine-spec-reporter": "^3.2.0",
    "loader-utils": "^1.1.0",
    "magic-string": "^0.22.2",
    "marked-terminal": "^2.0.0",
    "minimist": "^1.2.0",
    "npm-run": "^4.1.2",
    "npm-run-all": "^4.0.0",
    "rxjs": "^5.0.1",
    "semver": "^5.3.0",
    "source-map": "^0.5.6",
    "tar": "^3.1.5",
    "temp": "^0.8.3",
    "ts-node": "^2.0.0",
    "tslint": "^4.0.2",
    "typescript": "~2.3.0",
    "v8-profiler": "^5.7.0"
  },
  "devDependencies": {
    "protobufjs": "5.0.0"
  }
}
